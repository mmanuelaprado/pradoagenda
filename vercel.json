{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(index.tsx|App.tsx|types.ts|constants.tsx|Sidebar.tsx|sw.js|manifest.json|icon-.*)",
      "destination": "/$1"
    },
    {
      "source": "/(services|views|components)/(.*)",
      "destination": "/$1/$2"
    },
    {
      "source": "/((?!api/|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}