
{
  "rewrites": [
    {
      "source": "/manifest.json",
      "destination": "/manifest.json"
    },
    {
      "source": "/sw.js",
      "destination": "/sw.js"
    },
    {
      "source": "/favicon/(.*)",
      "destination": "/favicon/$1"
    },
    {
      "source": "/((?!api|manifest\\.json|sw\\.js|favicon/|.*\\.).*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
